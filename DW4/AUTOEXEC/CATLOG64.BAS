10 CLEAR 500
15 '
20 CLS
30 INPUT "Starting DSK:";SD
60 T=17:S=17
90 FOR D=SD TO 3583
100 DRIVE,D
110 PRINT "DSK";D;"- ";
120 DSKI$ 0,T,S,S1$,S2$
130 P=INSTR(1,S1$,(CHR$(0)))
140 IF P=0 THEN L$="N/A":GOTO 160
141 IF P>54 THEN P=54
150 L$=LEFT$(S1$,(P-1))
160 PRINT L$
165 R=R+1
166 IF R=31 GOTO 300
190 NEXT D
210 PRINT
220 PRINT "Done!"
230 END
300 PRINT "<press 'Q' to quit or any other key to continue>"
310 K$=INKEY$:IF K$="Q" GOTO 210
311 IF K$="" GOTO 310
320 R=1
330 GOTO 190
